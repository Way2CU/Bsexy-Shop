<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div class="list_item">
		<span class="column" style="width: 250px;">
			<cms:var param="name" multilanguage="yes"/>
		</span>
		<span class="column" style="width: 60px;">
			<cms:var param="price"/>
		</span>
		<span class="column" style="width: 60px;">
			<cms:var param="views"/>
		</span>
		<span class="column" style="width: 50px;">
			<cms:var param="rating"/>
		</span>
		<span class="column" style="width: 100px;">
			<cms:var name="date(Language::get_text('format_date_short'), $params['expires'])"/>
		</span>
		<span class="column">
			<cms:var param="item_change"/><cms:raw text=" "/>
			<cms:var param="item_delete"/><cms:raw text=" "/>
			<cms:var param="item_images"/>
		</span>
	</div>
</document>
